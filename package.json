{
  "name": "bibteximport",
  "productName": "Bibtex Import",
  "version": "1.0.1",
  "homepage": "http://focus.github.io/BibtexImport",
  "main": "lib/main.js",
  "application": {
    "name": "Bibtex Import"
  },
  "dependencies": {
    "bootstrap": "^3.0.0",
    "jquery": "^2.0.0",
    "nconf": "^0.8.2",
    "request": "^2.67.0"
  },
  "devDependencies": {
    "coffee-script": "^1.8.0",
    "electron-prebuilt": "^0.36.0"
  },
  "scripts": {
    "start": "./scripts/compile.sh && electron ./",
    "build": "./scripts/build.sh",
    "clean:dist": "rm -R ./dist",
    "clean": "rm -R ./dist ./bins",
    "package:osx": "electron-builder bins/BibtexImport-darwin-x64/BibtexImport.app --platform=osx --out=dist/osx64 --config=support/installer.json ",
    "package:win32": "electron-builder bins/BibtexImport-win32-ia32/ --platform=win --out=dist/win32 --config=support/installer.json ",
    "package:win64": "electron-builder bins/BibtexImport-win32-x64/ --platform=win --out=dist/win64 --config=support/installer.json",
    "package": "npm run clean:dist && npm run package:osx && npm run package:win32 && npm run package:win64"
  }
}
